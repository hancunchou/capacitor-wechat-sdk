{
  "name": "@hancunchou/capacitor-wechat-sdk",
  "version": "0.0.1",
  "description": "wechat sdk wrapper with capacitor ,support ios and android",
  "main": "dist/plugin.cjs.js",
  "module": "dist/esm/index.js",
  "types": "dist/esm/index.d.ts",
  "unpkg": "dist/plugin.js",
  "files": [
    "android/src/main/",
    "android/build.gradle",
    "dist/",
    "ios/Plugin/",
    "CapacitorWechatSdk.podspec"
  ],
  "author": "ChenQingze,Ertao",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/hancunchou/capacitor-wechat-sdk.git"
  },
  "homepage": "https://github.com/hancunchou/capacitor-wechat-sdk#readme",
  "bugs": {
    "url": "https://github.com/hancunchou/capacitor-wechat-sdk/issues"
  },
  "keywords": [
    "capacitor",
    "plugin",
    "native",
    "wechat"
  ],
  "scripts": {
    "verify": "npm run verify:ios && npm run verify:android && npm run verify:web",
    "verify:ios": "cd ios && pod install && xcodebuild -workspace Plugin.xcworkspace -scheme Plugin && cd ..",
    "verify:android": "cd android && ./gradlew clean build test && cd ..",
    "verify:web": "npm run build",
    "lint": "npm run eslint && npm run prettier -- --check && npm run swiftlint -- lint",
    "fmt": "npm run eslint -- --fix && npm run prettier -- --write && npm run swiftlint -- --fix --format",
    "eslint": "eslint . --ext ts",
    "prettier": "prettier \"**/*.{css,html,ts,js,java}\"",
    "swiftlint": "node-swiftlint",
    "docgen": "docgen --api WechatSDKPlugin --output-readme README.md --output-json dist/docs.json",
    "build": "npm run clean && npm run docgen && tsc && rollup -c rollup.config.js",
    "clean": "rimraf ./dist",
    "watch": "tsc --watch",
    "prepublishOnly": "npm run build"
  },
  "devDependencies": {
    "@capacitor/android": "6.2.0",
    "@capacitor/core": "^6.2.0",
    "@capacitor/docgen": "0.0.18",
    "@capacitor/ios": "6.2.0",
    "@ionic/eslint-config": "0.3.0",
    "@ionic/prettier-config": "1.0.1",
    "@ionic/swiftlint-config": "1.1.2",
    "eslint": "7.32.0",
    "prettier": "2.2.1",
    "prettier-plugin-java": "1.0.2",
    "rimraf": "^3.0.2",
    "rollup": "2.79.1",
    "swiftlint": "1.0.1",
    "typescript": "~5.4.0"
  },
  "peerDependencies": {
    "@capacitor/core": "^6.2.0"
  },
  "prettier": "@ionic/prettier-config",
  "swiftlint": "@ionic/swiftlint-config",
  "eslintConfig": {
    "extends": "@ionic/eslint-config/recommended"
  },
  "capacitor": {
    "ios": {
      "src": "ios"
    },
    "android": {
      "src": "android"
    }
  }
}
